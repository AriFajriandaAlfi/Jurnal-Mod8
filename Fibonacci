#include <iostream>
#include <stdio.h>

using namespace std;

int main()
{
    int bil[100];
    int x;
    bil[0]=1;
    bil[1]=1;
    int sum=2;
    cout << "Input : " << endl;
    cin >> x;
    for (int i=2; i<x;i++){
        bil[i]=bil[i-1]+bil[i-2];
        sum=bil[i]+sum;
    }
    cout << "Fibonacci : " << endl;
    cout << sum;
    return 0;
}

